{
  "name": "vandadsoft/eloquent-viewable",
  "description": "Laravel package that allows you to associate views with Eloquent models",
  "type": "library",
  "license": "MIT",
  "homepage": "https://github.com/vandadsoft/eloquent-viewable",
  "keywords": [
    "vandadsoft",
    "laravel",
    "eloquent",
    "views",
    "viewable",
    "visits",
    "visitable",
    "hits",
    "counter"
  ],
  "authors": [
    {
      "name": "Vandad Soft",
      "email": "vandadsoft.ir@gmail.com",
      "homepage": "https://vandadsoft.ir",
      "role": "Developer"
    }
  ],
  "require": {
    "php": "^7.3",
    "illuminate/cache": "^6.0|^7.0|^8.0",
    "illuminate/contracts": "^6.0|^7.0|^8.0",
    "illuminate/cookie": "^6.0|^7.0|^8.0",
    "illuminate/database": "^6.0|^7.0|^8.0",
    "illuminate/http": "^6.0|^7.0|^8.0",
    "illuminate/support": "^6.0|^7.0|^8.0",
    "jaybizzle/crawler-detect": "^v1.2",
    "nesbot/carbon": "^2.39"
  },
  "require-dev": {
    "illuminate/config": "^6.0|^7.0|^8.0",
    "mockery/mockery": "^1.4",
    "orchestra/testbench": "^4.0|^5.0|v6.0",
    "phpunit/phpunit": "^9.3"
  },
  "autoload": {
    "psr-4": {
      "Vandadsoft\\EloquentViewable\\": "src/"
    },
    "files": [
      "src/helpers.php"
    ]
  },
  "autoload-dev": {
    "psr-4": {
      "Vandadsoft\\EloquentViewable\\Tests\\": "tests/"
    }
  },
  "scripts": {
    "test": "vendor/bin/phpunit"
  },
  "config": {
    "sort-packages": true
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "extra": {
    "laravel": {
      "providers": [
        "Vandadsoft\\EloquentViewable\\EloquentViewableServiceProvider"
      ]
    }
  }
}
